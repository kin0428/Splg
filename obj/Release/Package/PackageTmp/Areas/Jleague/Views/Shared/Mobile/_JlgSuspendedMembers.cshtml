@if (Model.Count > 0)
{ 
    <dl class="socStartersList-sub">
        <dt>出場停止選手</dt>
        @foreach (var m in Model)
        {
            <dd>
                <p>
                    <span>@m.Pos</span>
                    <span>@m.Uni</span>
                    <span><a href="@Url.Action("Index", "JlgTeamInfoPlayerDetail", new { area = "Jleague", playerID = m.ID }, null)">@m.Name</a></span>
                </p>
            </dd>
        }
    </dl>
}